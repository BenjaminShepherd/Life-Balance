<div class="col l4 s10 offset-m1">
  <section class="category">

    <div class="card z-depth-4">
      <ul class="collection with-header">

        <li class="collection-header z-depth-2">
          <h5 class="category-title">Work</h5>
          {{> options-block}}
        </li>

        {{> task-block}}

        <li class="add-todo-item collection-item" data-category="{{category}}" style='display: none;'>
          <input type='text' class="new-task">
        </li>

        <a href=""><span class="sm-link">View All</span></a>
      </ul>
    </div>

    <div class="btn-wrapper">
      <a class="new-task-btn btn-floating btn-large waves-effect waves-light z-depth-4" data-category="{{category}}"><i
          class="material-icons">add</i></a>
    </div>
  </section>
</div>